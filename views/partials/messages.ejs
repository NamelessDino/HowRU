<% if(typeof errors != 'undefined') { %>
<% console.log(errors) %>
<% errors.forEach(error => { %>
<div class="alert alert-warning" role="alert">
    <%= error.msg %>
</div>
<% }); %>
<% } %>

<% if(success_msg != '') { %>

<div class="alert alert-success" role="alert">
    <%= success_msg %>
</div>

<% } %>

<% if(error_msg != '') { %>
<div class="alert alert-warning" role="alert">
    <%= error_msg %>
</div>
<% } %>

<% if (messages.error) { %>
<div class="alert alert-warning" role="alert">
    <%= messages.error %>
</div>
<% } %>